{
  "name": "@aztec/foundation",
  "packageManager": "yarn@3.4.1",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./src/index.js",
    "./eslint": "./eslintrc.js",
    "./prettier": "./.prettierrc.js"
  },
  "scripts": {
    "build": "yarn clean && yarn formatting && tsc -b tsconfig.dest.json",
    "build:dev": "tsc -b tsconfig.dest.json --watch",
    "clean": "rm -rf ./dest .tsbuildinfo",
    "formatting": "yarn run -T prettier --check ./src && yarn run -T eslint --max-warnings 0 ./src",
    "test": "NODE_NO_WARNINGS=1 node --experimental-vm-modules $(yarn bin jest) --no-cache"
  },
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "globals": {
      "ts-jest": {
        "useESM": true
      }
    },
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "testRegex": "./src/.*\\.test\\.ts$",
    "rootDir": "./src"
  },
  "dependencies": {
    "@aztec/barretenberg": "^2.1.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.4.3",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.7.23",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
